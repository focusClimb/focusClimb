{
  "manifest_version": 2,
  "name": "FocusClimb",
  "short_name": "FocusClimb",
  "version": "0.4.0",
  "description": "Replace new tab page with a personal dashboard featuring, desired pictures and pomodoro timer.",
  "icons": {
    "48": "focusClimb-48.png",
    "96": "focusClimb-96.png"
  },
  "chrome_url_overrides": {
    "newtab": "dashboard.html"
  },
  "browser_action": {
    "default_title": "FocusClimb"
  },
  "chrome_settings_overrides": {
    "homepage": "dashboard.html"
  }
} 




